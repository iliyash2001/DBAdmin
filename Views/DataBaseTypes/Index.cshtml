@model IEnumerable<DBAdmin.DataBaseType>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DataBaseTypes }) |
            @Html.ActionLink("Details", "Details", new { id=item.DataBaseTypes }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.DataBaseTypes })
        </td>
    </tr>
}

</table>
